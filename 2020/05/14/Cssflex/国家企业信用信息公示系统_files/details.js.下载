	function shareHolderAltDetail(altId){
		if(shareHolderAltTemp == false){
		$.ajax({
			type:"GET",
			url:judiciaryAltershareholderUrl,
			dataType:"JSON",
			async: true,
			success : function(data){
				console.log('shareHolderAltDetail');
				console.log(data);
				for(var i=0;i<data.data.length;i++){
					if(data.data[i].altId == altId){
						
						var executeDate = convertDate(data.data[i].executeDate);
						$("#copyright_alter_froAuth").html(data.data[i].froAuth);
						$("#copyright_alter_froDocNo").html(data.data[i].froDocNo);
						$("#copyright_alter_executeNo").html(data.data[i].executeNo);
						$("#copyright_alter_invType_CN").html(data.data[i].invType_CN);
						$('#copyright_alter_inv').html(data.data[i].inv);
						if(data.data[i].invCerType_CN != ""){
							$("#copyright_alter_invCerType_CN").html(data.data[i].invCerType_CN);
							//$("#copyright_alter_invCerNo").html(data.data[i].invCerNo);
						}else if(data.data[i].invCerType_CN == "" && data.data[i].invBLicType_CN != ""){
							$("#copyright_alter_invCerType_CN").html(data.data[i].invBLicType_CN);
							$("#copyright_alter_invCerNo").html(data.data[i].invBLicNo);
						}else{
							$("#copyright_alter_invCerType_CN").html("");
							$("#copyright_alter_invCerNo").html("");
						}
						$("#copyright_alter_froAm").html(data.data[i].froAm+"万"+data.data[i].regCapCur_CN);
						//$("#copyright_alter_regCapCur_CN").html(data.data[i].regCapCur_CN);
						//$("#copyright_alter_alienType_CN").html(data.data[i].alienType_CN);
						$("#copyright_alter_Alien").html(data.data[i].alien);
						if(data.data[i].alienCerType_CN != ""){
							$("#copyright_alter_AlienCerType_CN").html(data.data[i].alienCerType_CN);
							$("#copyright_alter_alienCerNo").html(data.data[i].alienCerNo);
						}else if(data.data[i].alienCerType_CN == "" && data.data[i].alienBLicType_CN != ""){
							$("#copyright_alter_AlienCerType_CN").html(data.data[i].alienBLicType_CN);
							$("#copyright_alter_alienCerNo").html(data.data[i].alienBLicNo);
						}else{
							$("#copyright_alter_AlienCerType_CN").html("");
							$("#copyright_alter_alienCerNo").html("");
						}
						$("#copyright_alter_executeDate").html(executeDate);
						$("#copyright_alter_thing").html("强制转让被执行人股权，办理有限责任公司股东变更登记");
					}
				}
			}
		})
		shareHolderAltTemp = true;
		}
		//$(".copyright_alter").show();
        document.getElementById("bg").style.display ="block";
        document.getElementById("copyright_alter").style.display ="block";
	}
	
	function licenceDetail(licId){
		if(licenceTemp == false){
		$.ajax({
			type:"GET",
			url:otherLicenceDetailInfoUrl,
			dataType:"JSON",
			async: true,
			success : function(data){
				for(var i=0;i<data.data.length;i++){
					if(data.data[i].licId == licId){
						var altDate = convertDate(data.data[i].altDate);
						$("#licence_alter_alt").html(data.data[i].alt);
						$("#licence_alter_altDate").html(altDate);
						$("#licence_alter_altBe").html(data.data[i].altBe);
						$("#licence_alter_altAf").html(data.data[i].altAf);
					}
				}
			}
		})
	}
		//$("#licence_alter").show();
        document.getElementById("bg").style.display ="block";
        document.getElementById("licence_alter").style.display ="block";
	}
	
