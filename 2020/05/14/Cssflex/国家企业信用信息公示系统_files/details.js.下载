						if(data.data[i].morReg_Id == morregId){
							var pefPerForm = convertDate(data.data[i].pefPerForm);
							var pefPerTo = convertDate(data.data[i].pefPerTo);
							//$("#priClaSec").append("<tr class='temp'><td class='entry'>"+data.data[i].priClaSecKind_CN+"</td><td class='entry'>"+data.data[i].priClaSecAm+"</td><td class='entry'>"+data.data[i].warCov+"</td><td class='entry'>"+pefPerForm+"</td><td class='entry'>"+pefPerTo+"</td><td class='entry'>"+data.data[i].remark+"</td></tr> ")
							//$("#mortgage_alter_priClaSecKind_CN").html("").html(data.data[i].priClaSecKind_CN);
