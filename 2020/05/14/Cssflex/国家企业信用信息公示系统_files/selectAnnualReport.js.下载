								}else if(row.alitem == "企业电子邮箱" || row.alitem == "电子邮箱"){
									$("#emailForColor1").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>企业电子邮箱：</a>");
									$("#emailForColor2").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>电子邮箱：</a>");
									$("#emailForColor3").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>电子邮箱：</a>");
								}else if(row.alitem == "从业人数" || row.alitem == "成员人数"){
									$("#empNumForColor1").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>从业人数：</a>");
									$("#empNumForColor2").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>从业人数：</a>");
									$("#empNumForColor3").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>成员人数：</a>");
									$("#empNumForColor4").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>从业人数：</a>");
								}else if(row.alitem == "经营状态"){
									$("#busStForColor1").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>企业经营状态：</a>");
