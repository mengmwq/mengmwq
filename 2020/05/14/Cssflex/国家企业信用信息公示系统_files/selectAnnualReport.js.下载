						$("#netIncDisIs").html("").html("企业选择不公示");
					}
					//纳税总额是否公示
					var ratGroDisIs = data.data[0].ratGroDis;
					if(ratGroDisIs==1){
						$("#ratGroDisIs").html("").html(data.data[0].ratGro);
					}else{
						$("#ratGroDisIs").html("").html("企业选择不公示");
					}
					//负债总额是否公示
