							return "<tr>"+row.alitem+"</tr>";
						}
					},{
			   	 		"targets" : [0],
						"render":function(data, x, obj, index, y){
							return index.row + index.settings._iDisplayStart + 1;
						} 
					}]
				});
			}
		}
		
		
		//默认网店信息为  否
		$("#webIsOrNot").html("否");
		
		//是否有网站网店信息
		var dataForWeb =null;
		//年报中网站网店信息  全部信息
		if(webSiteInfoUrl!= null){
			$.ajax({
				url : webSiteInfoUrl+"?entType="+ent_type_result,
				async : false,
				success : function(data){
					if(data.totalPage > 1){
						$("#annualWebAll").html("").html("<span id='annualWebColor' >网站或网店信息</span> <a href='"+allWebInfoModelUrl+"?entType="+ent_type_result+"' target='_blank' style='float:right;color:#804000;'>查看全部 &gt;&gt;</a>");
					}else{
						$("#annualWebAll").html("").html("<span id='annualWebColor' >网站或网店信息</span>");
					}
					dataForWeb = data.data ;
					if(dataForWeb != null && dataForWeb.length > 0){
						$("#webIsOrNot").html("是");
					}else{
						$("#webIsOrNot").html("否");
					}
					if(data != null && data.data.length > 0){
						var str = "";
						//格子形式网站网店信息
						if(data.data.length > 0 ){
							for(var i =0;i<data.data.length;i++){
								var strForWeb = "";
								if(data.data[i].webSitName != null && data.data[i].webSitName!= "" && data.data[i].webSitName.length > 0){
									if(data.data[i].webSitName.length > 14){
										strForWeb = data.data[i].webSitName.substring(0,14);
										strForWeb+="...";
									}else{
										strForWeb = data.data[i].webSitName;
									}
								}
								var strForDomain = "";
								if(data.data[i].domain != null && data.data[i].domain!= "" && data.data[i].domain.length > 0){
									if(data.data[i].webSitName.length > 18){
										strForDomain = data.data[i].domain.substring(0,18);
										strForDomain+="...";
									}else{
										strForDomain = data.data[i].domain;
									}
								}
								str = str + '<li><a><div style="font-size:14px;color:#333333;" title="'+data.data[i].webSitName+'">'+strForWeb+
										'</div><span style="font-size:12px;color:#666666;">· 类型 : '
										+'<span style="font-size:12px;color:#333333;" title='+(data.data[i].webType==1?"网站":"网店")+'>'+(data.data[i].webType==1?"网站":"网店")+'</span>'+
										'</span><div style="font-size:12px;color:#666666;position:relative;top:-10px;">· 网址 : '+
										'<span style="font-size:12px;color:#333333;" title="'+data.data[i].domain+'">'+strForDomain+'</span></div></a></li>';
							}
							$("#webSiteList").html("").html(str);
						}
					}else{
						$("#webSiteList").html("").html("<tr class='odd'><td  style='background:#F3F7FA;' width='1077px' id='dataTables_empty' colspan='4'>暂无网站或网店信息</td></tr>");
					}
					
				}
			});
		}

		//初始化  对外投资信息 变量
		var forinvestmentforhome = null;
		
		//默认是否有投资信息或购买其他公司股权  否
		$("#forInvestment").html("否");
		
		//年报中对外投资信息  全部信息
		if(forInvestmentUrl != null){
		$.ajax({
			url : forInvestmentUrl ,
			async : false,
			success : function(data){
				if(data.totalPage > 1){
					$("#annualToOutAll").html("").html("<span id='annualToOutColor' >对外投资信息</span><a href='"+allForgModelUrl+"' target='_blank' style='float:right;color:#804000;'>查看全部 &gt;&gt;</a>");
				}else{ 
					$("#annualToOutAll").html("").html("<span id='annualToOutColor' >对外投资信息</span>");
				}
				forinvestmentforhome = data.data;
				if(forinvestmentforhome != null && forinvestmentforhome.length > 0){
            		$("#forInvestment").html("是");
				}else{
					$("#forInvestment").html("否");
				}
				//格子形式  对外投资信息
				if(data!= null && data.data.length > 0){
					var str = "";
					for(var i = 0; i< data.data.length;i++){
						var strForEntName = "";
						if(data.data[i].entName != "" && data.data[i].entName != null && data.data[i].entName.length > 0){
							if(data.data[i].entName.length > 22){
								strForEntName = data.data[i].entName.substring(0,22);
								strForEntName+="...";
							}else{
								strForEntName = data.data[i].entName;
							}
						}
						
						var strForUnscId ="";
						if(data.data[i].uniscId != "" && data.data[i].uniscId != null && data.data[i].uniscId.length > 0){
							if(data.data[i].uniscId.length > 14){
								strForUnscId = data.data[i].uniscId.substring(0,14);
								strForUnscId+="...";
							}else{
								strForUnscId = data.data[i].uniscId;
							}
						}
						
						str = str + '<li><a><div style="font-size:14px;color:#333333;" title='+data.data[i].entName+'>'
							+''+strForEntName+
							'</div><span style="font-size:12px;color:#666666;">· 统一社会信用代码/注册号 : '
							+'<span style="font-size:12px;color:#333333;" title='+data.data[i].uniscId+'>'+strForUnscId+'</span></span></a></li>';
					}
					$("#forInvsmentList").html("").html(str);
				}else{
					$("#forInvsmentList").html("").html("<tr class='odd'><td  style='background:#F3F7FA;' width='1077px' id='dataTables_empty' colspan='4'>暂无对外投资信息</td></tr>");
				}
			}
		});
	}
		
		
		
		
		
	}
	