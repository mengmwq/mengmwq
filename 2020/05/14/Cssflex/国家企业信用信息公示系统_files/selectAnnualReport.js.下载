	//年报年报的点击年份的js
function changeYearForm(anCheIdJs,entTypeJs,anCheYearJs,entTypeResultForJs){
		var arrayList = anCheIdJs.split(",");
		if(arrayList != null){
			anCheIdJs = arrayList[0];
			entTypeJs = arrayList[1];
			anCheYearJs = arrayList[2];
			entTypeResultForJs = arrayList[3];
		}
		
		var entTypeResult = parseInt(entTypeResultForJs);
		if((entTypeJs == "e") && (entTypeResult >0 && entTypeResult < 18)){
						$.ajax({
							type:"GET",
							url:VAnnualReportBaseinfoUrl+anCheIdJs+".html",
							dataType:"JSON",
							async: false,
							success : function(data){
								var json = eval(data);
								$("#uniscId").html("").html(json.uniscId);
								$("#entName").html("").html(json.entName);
								$("#tel").html("").html(json.tel);
								$("#addr").html("").html(json.addr);
								$("#postalCode").html("").html(json.postalCode);
								$("#email").html("").html(json.email);
								$("#busSt").html("").html(json.busSt_CN);
								if(json.empNumDis == "2"){
									$("#empNum").html("").html("企业选择不公示");
								}else{
									$("#empNum")
									.html("").html(
											json.empNum);
								}
								//隶属企业名称
								$("#dependentEntName").html("").html(json.dependentEntName);
								if(json.anCheYear != null && (entTypeResult != 18)){
									$("#reportName").html("").html(json.anCheYear+"年度报告" +"<select style='float:left;position:relative;top:10px;font-family:"+YaHei+"' class='f14 ml10' id='AnCheYearAnnual' onChange='changeYearForm(this.value)'><option>" +
											"选择年份报告</option></select><span>"
								               +" <span class='report_span pl5 pr5 mr10' id='Annual_modify' style='font-size:12px;position:relative;bottom:10px;'>"
							                   +" </span>"
							                   +"  <span style='float:right;font-size:14px;color:#999;position:relative;top:10px;' >填报时间:<span id='anCheDate'></span></span>");
									forAnnualDetail("e");
								}else{
									$("#reportName").html("").html("无");
								}
								var  javascriptanChedate = convertDate(json.anCheDate);

								$("#anCheDate").html("").html(javascriptanChedate);
								var javascriptDate = convertDate(json.anCheDate);
								if((entTypeResult != 18)){
									$("#reportFillIn").html("").html(javascriptDate);
								}else{
									$("#reportFillIn").html("").html("无");
								}
								
								//获取anCheId
								var anCheId = json.anCheId;
								
								$.ajax({
									type:"GET",
									url:vAnnualReportBaseInfoForJsUrl+anCheId+".html",
									dataType:"JSON",
									async: false,
									success : function(data){
										var json = eval(data);
										webSiteInfoUrl = json.webSiteInfoUrl;
										sponsorUrl = json.sponsorUrl;
										forInvestmentUrl = json.forInvestmentUrl;
										baseinfoUrl = json.baseinfoUrl;
										alterUrl = json.alterUrl;
										vAnnualReportAlterstockinfoUrl = json.vAnnualReportAlterstockinfoUrl;
										forGuaranteeinfoUrl = json.forGuaranteeinfoUrl;
										alterTotalPageUrl = json.alterTotalPageUrl;
										vAnnualReportBranchProductionUrl = json.vAnnualReportBranchProductionUrl;
										forGuaranteeinfoOrNoUrl = json.forGuaranteeinfoOrNoUrl;
										allWebInfoUrl = json.allWebInfoUrl;
										allForinvestmentInfoUrl = json.allForinvestmentInfoUrl;
										allWebInfoModelUrl = json.allWebInfoModelUrl;
										allForgModelUrl= json.allForgModelUrl;
										$.ajax({
											type:"GET",
											url:alterTotalPageUrl+"?entType="+entTypeResult,
											dataType:"JSON",
											async: false,
											success : function(data){
												var json = eval(data);
												var intEntType = parseInt(entTypeResult);
												if((intEntType != 18)){
													if(anCheYearJs >= 2016){
														$("#Annual_modify").html("").html("<a href='#modifyMark' style='color:#ed1d4a;'>"+
																json.recordsTotal
																+ "  条修改记录</a>");
													}else{
														$("#Annual_modify").html("").html("<a href='#modifyMark'  style='color:#ed1d4a;'>"+
																json.recordsTotal
																+ "  条修改记录</a>");
													}
												}
											}
											
										})
									}
								});
							}
						});	
		}else if(entTypeJs == "sfc"){
				$.ajax({
					type:"GET",
					url:VAnnualReportSfcBaseinfoUrl+anCheIdJs+".html",
					dataType:"JSON",
					async: false,
