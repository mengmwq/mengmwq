        $.ajax({
            type: "POST",
            url: vAnnualReportBranchProductionUrl,
            dataType: "JSON",
            async: false,
            success: function (data) {
                if(data && data.data){
                    var data = data.data[0];
                    var tds = $('#needPaging_branchProduction').find('td');
                    if(data.maiBusIncDis==1){
                        tds.eq(1).html(Math.round(data.maiBusInc));
                    }else{
                        tds.eq(1).html('企业选择不公示');
                    }
                    if(data.ratGroDis==1){
                        tds.eq(3).html(Math.round(data.ratGro));
                    }else{
                        tds.eq(3).html('企业选择不公示');
                    }
                    if(data.netIncDis==1){
                        tds.eq(5).html(Math.round(data.netInc));
                    }else{
                        tds.eq(5).html('企业选择不公示');
                    }
                }else{
                   $('#needPaging_branchProduction').remove();
                   $('#onePbAss').append('<p>暂无生产经营状态信息</p>')
                }

            }   
        });
