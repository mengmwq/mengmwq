	function shareHodlerDetail(inv, invId){
        $(".entry").remove();
		var shareHodlerDetailUrl = "/corp-query-entprise-info-shareholderDetail-"+invId+".html";
		if(shareHolderTemp == false){
		$.ajax({
			type:"GET",
			url:shareHodlerDetailUrl,
			dataType:"JSON",
			async: true,
			success : function(data){
				var entType = $('#entType').val();
				if(entType==101 ||entType==1001){
					$('.detail-title').html('发起人及出资详细信息');
					$('.shareholders_details #onemortgage_alter_info').html('发起人信息');
					$('#shareholders_details_inv').prev().html('发起人');
				}else if(entType==3){
					$('.detail-title').html('主管部门（出资人）详细信息');
					$('.shareholders_details #onemortgage_alter_info').html('主管部门（出资人）信息');
					$('#shareholders_details_inv').prev().html('主管部门（出资人）');
				}else if(entType==15){
					$('.detail-title').html('合作各方详细信息');
					$('.shareholders_details #onemortgage_alter_info').html('合作方信息');
					$('#shareholders_details_inv').prev().html('合作方');
				}else if(entType==7){
					$('.detail-title').html('投资人详细信息');
					$('.shareholders_details #onemortgage_alter_info').html('投资人信息');
					$('#shareholders_details_inv').prev().html('投资人');
				}else if(entType==5 ||entType==12){
					$('.detail-title').html('合伙人详细信息');
					$('.shareholders_details #onemortgage_alter_info').html('合伙人信息');
					$('#shareholders_details_inv').prev().html('合伙人');
				}else{

				}
				var num = 0;
				var numTemp = 0;
				var liSubConAm = 0;
				var liAcConAm = 0;
				var temp = 0;
				for(var i=1;i<data.data.length;i++){					
					numTemp = data.data[i].length;
					if(numTemp<temp){
						numTemp = temp;
					}
					temp = numTemp;
				}
				
				if(temp > num){					
					num = temp;
				}
				
				$(".row").attr("rowSpan", num);
				var start = 0;
				var liAcConAm2 = "";
				var liAcConAmCount = "";
				for(var i=0;i<data.data[0].length;i++){
					liAcConAm = FloatAdd(liAcConAm , data.data[0][i].acConAm);
					liAcConAm2 = liAcConAm.toString();
					start = liAcConAm2.indexOf(".");
					if(start>0){
						liAcConAmCount = liAcConAm2.substring(0,start+7);
					}else{
						liAcConAmCount = liAcConAm2;
					}
				}
				var liSubConAm2 = "";				
				var liSubConAmCount = "";
				for(var i=0;i<data.data[1].length;i++){
					liSubConAm = FloatAdd(liSubConAm , data.data[1][i].subConAm);
					 liSubConAm2 = liSubConAm.toString();
					 start = liSubConAm2.indexOf(".");
					 if(start>0){
						 liSubConAmCount = liSubConAm2.substring(0,start+7);
					 }else{
						 liSubConAmCount = liSubConAm2;
					 }
				}
				$("#shareholders_details_inv").html(inv.replace(/&rsquo;/,"'"));
				$("#shareholders_details_liSubConAm").html(liSubConAmCount);
				$("#shareholders_details_liAcConAm").html(liAcConAmCount);
				//认缴
				if(data.data[1].length > 0){
					$("#shareholders_details_sub").after("");
					for(var i=0;i<data.data[1].length;i++){
						if(data.data[1][i].conDate == null){
							var subConDate = "";
							var subam = "";
							var comForm ="";
						}else{
							var subConDate = convertDate(data.data[1][i].conDate);
							var subam = data.data[1][i].subConAm;
							var comForm = data.data[1][i].conForm_CN;
						}																	
						$("#shareholders_details_sub").after("<tr><td class='entry'>"+comForm+"</td><td class='entry'>"+subam+"</td><td class='entry'>"+subConDate+"</td></tr>");
					}			
				}else{
					var	subConDate = "";
					var subam = "";
					var comForm = "";
				}
				//实缴			
				if(data.data[0].length > 0){	
					$("#shareholders_details_paid").after("");
					for(var j=0;j<data.data[0].length;j++){
						if(data.data[0][j].conDate == null){
							var conDate = "";
							var acAm= "";
							var acComfrom = "";
						}else{
							var conDate = convertDate(data.data[0][j].conDate);
							var acAm = data.data[0][j].acConAm;
							var acComfrom = data.data[0][j].conForm_CN;
						}						
						$("#shareholders_details_paid").after("<tr><td class='entry'>"+acComfrom+"</td><td class='entry'>"+acAm+"</td><td class='entry'>"+conDate+"</td></tr>");
					}					
				}else{
					var conDate = "";
					var acAm= "";
					var acComfrom = "";
				}									
			}
		})
		}
        document.getElementById("bg").style.display ="block";
        document.getElementById("shareholders_details").style.display ="block";
	}
	
