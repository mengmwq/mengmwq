var annualBranchInfo = null;
var annualLicenceInfo = null;
var vAnnualReportBranchProductionInfo = null;
var sponsorInfo = null;
var forGuaranteeinfo = null;
var alterEInfo = null;
var forGuaranteeinfoOrNoUrl = null;
var webSiteInfoUrl = null;
var sponsorUrl = null;
var forInvestmentUrl = null;
var baseinfoUrl = null;
//对外担保
var forGuaranteeinfoUrl = null;
//年报修改
var alterUrl = null;
//企业年报中 分支机构的生产经营信息
var vAnnualReportBranchProductionUrl = null;
var vAnnualReportAlterstockinfoUrl = null;
//农专分支机构
var vAnnualReportSfcBranchUrl = null;
//农专个体许可
var annulLicenceUrl = null;
//年报修改条数
var alterTotalPageUrl = null;
//sfc企业资产信息
var vannualSfcAssertUrl = null;
//pb企业资产信息
var vAnnPbAssetUrl = null;
//年份
var anCheYearJs = null;

//年报个体经营资产信息是否公示
var ratGroDis = null;

//全部网站网店信息   全部对外投资信息
var allWebInfoUrl = null;
var allForinvestmentInfoUrl = null;
//model  网页网站信息
var allWebInfoModelUrl = null;
var allForgModelUrl = null;


function showAnnualReport(entType) {

    //ajax请求  普通企业机构的年报基本信息

    //entType数字类型
    var entTypeResultForJs = entType;
    //网店网址

    var anCheIdJs = null;
    var entTypeJs = null;


    $.ajax({
        type: "POST",
        url: anCheYearInfo,
        dataType: "JSON",
        async: false,
        success: function (data) {
            // 判断是否为null
            if (data.length === 0) {
                //农专
                if (entTypeResultForJs == 17) {
                    $("#reportName").prepend(
                        "该农专社没有报送的年报信息");
                    $("span#Annual_modify").css(
                        "display", "none");
                    $("span#anCheDate").prepend("无");
                    $("#annualReportHidden").css(
                        "display", "none");
                } else if (entTypeResultForJs == 16) {
                    $("#reportName").prepend(
                        "该个体户没有报送的年报信息");
                    $("span#Annual_modify").css(
                        "display", "none");
                    $("span#anCheDate").prepend("无");
                    $("#annualReportHidden").css(
                        "display", "none");
                } else {
                    $("#reportName").prepend(
                        "该企业没有报送的年报信息");
                    $("span#Annual_modify").css(
                        "display", "none");
                    $("span#anCheDate").prepend("无");
                    $("#annualReportHidden").css(
                        "display", "none");
                }

            } else {
                // 取json list数据

                var json = eval(data);
                for (var i = 0; i < json.length; i++) {
                    anCheIdJs = json[i].anCheId;
                    entTypeJs = json[i].entType;
                    anCheYearJs = json[i].anCheYear;

                    // 条件判断entType
                    if ((entTypeResultForJs > 0 && entTypeResultForJs < 19)
                        && (entTypeResultForJs != 18)
                        && entTypeResultForJs == 16) {
                        if (entTypeJs == "pb") {
                            $("#AnCheYearAnnual")
                                .append(
                                    "<option class ='year'"
                                    + "value=" + anCheIdJs + "," + entTypeJs + "," + anCheYearJs + "," + entTypeResultForJs + ">"
                                    + anCheYearJs
                                    + "</option>");
                        }
                    } else if ((entTypeResultForJs > 0 && entTypeResultForJs < 19)
                        && (entTypeResultForJs != 18)
                        && entTypeResultForJs == 17) {
                        if (entTypeJs == "sfc") {
                            $("#AnCheYearAnnual")
                                .append(
                                    "<option class ='year'"
                                    + "value=" + anCheIdJs + "," + entTypeJs + "," + anCheYearJs + "," + entTypeResultForJs + ">"
                                    + anCheYearJs
                                    + "</option>");
                        }

                    } else if ((entTypeResultForJs > 0 && entTypeResultForJs < 19)
                        && (entTypeResultForJs != 18)) {
                        if (entTypeJs == "e") {
                            $("#AnCheYearAnnual")
                                .append(
                                    "<option class ='year'"
                                    + "value=" + anCheIdJs + "," + entTypeJs + "," + anCheYearJs + "," + entTypeResultForJs + ">"
                                    + anCheYearJs
                                    + "</option>");
                        }
                    }


                }
                // alert(anCheIdJs)
                // 如果ENTType -== 什么什么 就调用什么什么
                if ((entTypeJs == "e")
                    && (entTypeResultForJs > 0 && entTypeResultForJs < 19)
                    && entTypeResultForJs != 18
                    && anCheIdJs != null && !("undefined" == typeof(anCheIdJs))) {
                    $
                        .ajax({
                            type: "POST",
                            url: VAnnualReportBaseinfoUrl
                            + anCheIdJs
                            + ".html",
                            dataType: "JSON",
                            async: false,
                            success: function (data) {
                                var json = eval(data);
                                if (json.uniscId != null && json.uniscId != "") {
                                	$("#uniscId")
                                    .append(
                                        json.uniscId);
                                	$("#uniscIdForColor1")
                                    .html("统一社会信用代码");
                                } else {
                                	$("#uniscId")
                                    .append(
                                        json.regNo);
                                	$("#uniscIdForColor1")
                                    .html("注册号");
                                }
                                $("#entName")
                                    .append(
                                        json.entName);
                                $("#tel")
                                    .append(
                                        json.tel);
                                $("#addr")
                                    .append(
                                        json.addr);
                                $(
                                    "#postalCode")
                                    .append(
                                        json.postalCode);
                                $("#email")
                                    .append(
                                        json.email);
                                $("#busSt")
                                    .append(
                                        json.busSt_CN);
                                if (json.empNumDis == "2") {
                                    $("#empNum").append("企业选择不公示");
                                } else {
                                    $("#empNum")
                                        .append(
                                            json.empNum);
                                }
                                var javascriptanChedate = convertDate(json.anCheDate);

                                $("#anCheDate").append(javascriptanChedate);

                                // 隶属企业名称
                                $(
                                    "#dependentEntName")
                                    .append(
                                        json.dependentEntName);
                                if (json.anCheYear != null
                                    && (entTypeResultForJs != 18)) {
                                    $(
                                        "#reportName")
                                        .prepend(
                                            json.anCheYear
                                            + "年度报告");
                                } else {
                                    $(
                                        "#reportName")
                                        .prepend(
                                            "无年度报告");
                                }

                                var javascriptDate = convertDate(
                                    json.anCheDate);

                                if ((entTypeResultForJs != 18)) {
                                    $(
                                        "#reportFillIn")
                                        .prepend(
                                            javascriptDate);
                                } else {
                                    $(
                                        "#reportFillIn")
                                        .prepend(
                                            "无");
                                }

                                // 获取anCheId
                                var anCheId = json.anCheId;
                                $.ajax({
                                        type: "POST",
                                        url: vAnnualReportBaseInfoForJsUrl
                                        + anCheId
                                        + ".html",
                                        dataType: "JSON",
                                        async: false,
                                        success: function (data) {
                                            var json = eval(data);
                                            webSiteInfoUrl = json.webSiteInfoUrl;
                                            sponsorUrl = json.sponsorUrl;
                                            forInvestmentUrl = json.forInvestmentUrl;
                                            baseinfoUrl = json.baseinfoUrl;
                                            alterUrl = json.alterUrl;
                                            vAnnualReportAlterstockinfoUrl = json.vAnnualReportAlterstockinfoUrl;
                                            forGuaranteeinfoUrl = json.forGuaranteeinfoUrl;
                                            alterTotalPageUrl = json.alterTotalPageUrl;
                                            vAnnualReportBranchProductionUrl = json.vAnnualReportBranchProductionUrl;
                                            forGuaranteeinfoOrNoUrl = json.forGuaranteeinfoOrNoUrl;
                                            allWebInfoUrl = json.allWebInfoUrl;
                                            allForinvestmentInfoUrl = json.allForinvestmentInfoUrl;
                                            allWebInfoModelUrl = json.allWebInfoModelUrl;
                                            allForgModelUrl = json.allForgModelUrl;
                                            $
                                                .ajax({
                                                    type: "POST",
                                                    url: alterTotalPageUrl
                                                    + "?entType="
                                                    + entTypeResultForJs,
                                                    dataType: "JSON",
                                                    async: false,
                                                    success: function (data) {
                                                        var json = eval(data);
                                                        var intEntType = parseInt(entTypeResultForJs);
                                                        if ((intEntType != 18)) {
                                                            if (anCheYearJs >= 2016) {
                                                                $("#Annual_modify")
                                                                    .prepend("<a href='#modifyMark' style='color:#ed1d4a;'>" +
                                                                        json.recordsTotal
                                                                        + "  条修改记录</a>");
                                                            } else {
                                                                $("#Annual_modify")
                                                                    .prepend("<a href='#modifyMark'  style='color:#ed1d4a;'>" +
                                                                        json.recordsTotal
                                                                        + "  条修改记录</a>");
                                                            }

                                                        }

                                                    }

                                                })
                                        }
                                    });
                            }
                        });
                } else if (entTypeJs == "sfc"
                    && anCheIdJs != null && !("undefined" == typeof(anCheIdJs))) {
                    $
                        .ajax({
                            type: "POST",
                            url: VAnnualReportSfcBaseinfoUrl
                            + anCheIdJs
                            + ".html",
                            dataType: "JSON",
                            async: false,
                            success: function (data) {
                                var json = eval(data);
                                $(
                                    "#SfcuniscId")
                                    .append(
                                        +json.uniscId);
                                $(
                                    "#SfcfarSpeArtName")
                                    .append(
                                        json.farSpeArtName);
                                $(
                                    "#SfcmemNum")
                                    .append(
                                        json.memNum);
                                $(
                                    "#Sfcfarnum")
                                    .append(
                                        json.farnum);
                                $(
                                    "#SfcannNewMemNum")
                                    .append(
                                        json.annNewMemNum);
                                $(
                                    "#SfcannRedMemNum")
                                    .append(
                                        json.annRedMemNum);
                                $("#Sfctel")
                                    .append(
                                        json.tel);
                                var javascriptanChedate = convertDate(json.anCheDate);

                                $("#anCheDate").append(javascriptanChedate);
                                $("#Sfcemail")
                                    .append(
                                        json.email);
                                if (json.anCheYear != null
                                    && (entTypeResultForJs != 18)) {
                                    $(
                                        "#reportName")
                                        .prepend(
                                            json.anCheYear
                                            + "年度报告");
                                } else {
                                    $(
                                        "#reportName")
                                        .prepend(
                                            "无年度报告");
                                }

                                var javascriptDate = convertDate(
                                    json.anCheDate);

                                if ((entTypeResultForJs != 18)) {
                                    $(
                                        "#reportFillIn")
                                        .prepend(
                                            javascriptDate);
                                } else {
                                    $(
                                        "#reportFillIn")
                                        .prepend(
                                            "无");
                                }

                                var anCheId = json.anCheId;

                                $
                                    .ajax({
                                        type: "POST",
                                        url: vAnnualSfcReportBaseInfoForJsUrl
                                        + anCheId
                                        + ".html",
                                        dataType: "JSON",
                                        async: false,
                                        success: function (data) {
                                            var json = eval(data);
                                            webSiteInfoUrl = json.webSiteInfoUrl;
                                            baseinfoUrl = json.baseinfoUrl;
                                            alterUrl = json.alterUrl;
                                            vAnnualReportSfcBranchUrl = json.vAnnualReportSfcBranchUrl;
                                            annulLicenceUrl = json.annulLicenceUrl;
                                            alterTotalPageUrl = json.alterTotalPageUrl;
                                            vannualSfcAssertUrl = json.vannualSfcAssertUrl;
                                            //alert(webSiteInfoUrl);
                                            allWebInfoUrl = json.allWebInfoUrl;
                                            allForinvestmentInfoUrl = json.allForinvestmentInfoUrl
                                            allWebInfoModelUrl = json.allWebInfoModelUrl;

                                            $
                                                .ajax({
                                                    type: "POST",
                                                    url: alterTotalPageUrl
                                                    + "?entType="
                                                    + entTypeResultForJs,
                                                    dataType: "JSON",
                                                    async: false,
                                                    success: function (data) {
                                                        var json = eval(data);
                                                        var intEntType = parseInt(entTypeResultForJs);
                                                        if ((intEntType != 18)) {
                                                            if (anCheYearJs >= 2016) {
                                                                $("#Annual_modify")
                                                                    .prepend("<a href='#modifyMark' style='color:#ed1d4a;'>" +
                                                                        json.recordsTotal
                                                                        + "  条修改记录</a>");
                                                            } else {
                                                                $("#Annual_modify")
                                                                    .prepend("<a href='#modifyMark'  style='color:#ed1d4a;'>" +
                                                                        json.recordsTotal
                                                                        + "  条修改记录</a>");
                                                            }
                                                        }
                                                    }

                                                })
                                        }
                                    });
                            }
                        });
                } else if (entTypeJs == "pb"
                    && anCheIdJs != null && ("undefined" != typeof(anCheIdJs))) {
                    $
                        .ajax({
                            type: "POST",
                            url: VAnnualReportPbBaseinfoUrl
                            + anCheIdJs
                            + ".html",
                            dataType: "JSON",
                            async: false,
                            success: function (data) {
                                var json = eval(data);
                                console
                                    .log(json);
                                $(
                                    "#PbuniscId")
                                    .append(
                                        json.uniscId);
                                $(
                                    "#PbtraName")
                                    .append(
                                        json.traName);
                                $("#Pbname")
                                    .append(
                                        json.name);
                                $("#Pbtel")
                                    .append(
                                        json.tel);
                                $("#PbempNum")
                                    .append(
                                        json.empNum);
                                $("#PbfundAm")
                                    .append(
                                        json.fundAm+'万');
                                var javascriptanChedate = convertDate(json.anCheDate);

                                $("#anCheDate").append(javascriptanChedate);
                                if (json.anCheYear != null
                                    && (entTypeResultForJs != 18)) {
                                    $(
                                        "#reportName")
                                        .prepend(
                                            json.anCheYear
                                            + "年度报告");
                                } else {
                                    $(
                                        "#reportName")
                                        .prepend(
                                            "无年度报告");
                                }
                                var javascriptDate = convertDate(
                                    json.anCheDate);

                                if ((entTypeResultForJs != 18)) {
                                    $(
                                        "#reportFillIn")
                                        .prepend(
                                            javascriptDate);
                                } else {
                                    $(
                                        "#reportFillIn")
                                        .prepend(
                                            "无");
                                }
                                var anCheId = json.anCheId;
                                $
                                    .ajax({
                                        type: "POST",
                                        url: vAnnualPbReportBaseInfoForJsUrl
                                        + anCheId
                                        + ".html",
                                        dataType: "JSON",
                                        async: false,
                                        success: function (data) {
                                            var json = eval(data);
                                            webSiteInfoUrl = json.webSiteInfoUrl;
                                            alterUrl = json.alterUrl;
                                            vAnnualReportSfcBranchUrl = json.vAnnualReportSfcBranchUrl;
                                            annulLicenceUrl = json.annulLicenceUrl;
                                            alterTotalPageUrl = json.alterTotalPageUrl;
                                            vAnnPbAssetUrl = json.vAnnPbAssetUrl;
                                            allWebInfoUrl = json.allWebInfoUrl;
                                            allForinvestmentInfoUrl = json.allForinvestmentInfoUrl
                                            allWebInfoModelUrl = json.allWebInfoModelUrl;

                                            $
                                                .ajax({
                                                    type: "POST",
                                                    url: alterTotalPageUrl
                                                    + "?entType="
                                                    + entTypeResultForJs,
                                                    dataType: "JSON",
                                                    async: false,
                                                    success: function (data) {
                                                        var json = eval(data);
                                                        var intEntType = parseInt(entTypeResultForJs);
                                                        if ((intEntType != 18)) {
                                                            if (anCheYearJs >= 2016) {
                                                                $("#Annual_modify")
                                                                    .prepend("<a href='#modifyMark' style='color:#ed1d4a;'>" +
                                                                        json.recordsTotal
                                                                        + "  条修改记录</a>");
                                                            } else {
                                                                $("#Annual_modify")
                                                                    .prepend("<a href='#modifyMark'  style='color:#ed1d4a;'>" +
                                                                        json.recordsTotal
                                                                        + "  条修改记录</a>");
                                                            }
                                                        }
                                                    }

                                                })
                                        }
                                    });
                            }
                        });
                } else {
                    $("#reportName").prepend(
                        "该企业没有报送的年报信息");
                    $("#reportFillIn").prepend(
                        "无");
                }
            }
        }
    });


    var baseinfoColumns = [
        "assGro",
        "totEqu",
        "vendInc",
        "maiBusInc",
        "ratGro",
        "proGro",
        "netInc",
        "liaGro"
    ];

    //企业年报信息 资产信息是否公示
    $.ajax({
        type: "POST",
        url: baseinfoUrl,
        dataType: "JSON",
        async: false,
        success: function (data) {
            if (data.data != null && data.data.length > 0) {
                //资产总额是否公示
                var assGroDisIs = data.data[0].assGroDis;
                if (assGroDisIs == 1) {
                    $("#assGroDisIs").html("").html(data.data[0].assGro+" 万元");
                } else {
                    $("#assGroDisIs").html("").html("企业选择不公示");
                }
                //所有者权益合计是否公示
                var totEquDisIs = data.data[0].totEquDis;
                if (totEquDisIs == 1) {
                    $("#totEquDisIs").html("").html(data.data[0].totEqu+" 万元");
                } else {
                    $("#totEquDisIs").html("").html("企业选择不公示");
                }
                //营业总收入是否公示
                var vendIncDisIs = data.data[0].vendIncDis;
                if (vendIncDisIs == 1) {
                    $("#vendIncDisIs").html("").html(data.data[0].vendInc+" 万元");
                } else {
                    $("#vendIncDisIs").html("").html("企业选择不公示");
                }
                //利润总额是否公示
                var proGroDisIs = data.data[0].proGroDis;
                if (proGroDisIs == 1) {
                    $("#proGroDisIs").html("").html(data.data[0].proGro+" 万元");
                } else {
                    $("#proGroDisIs").html("").html("企业选择不公示");
                }
                //其中主营业务收入是否公示
                var maiBusIncDisIs = data.data[0].maiBusIncDis;
                if (maiBusIncDisIs == 1) {
                    $("#maiBusIncDisIs").html("").html(data.data[0].maiBusInc+" 万元");
                } else {
                    $("#maiBusIncDisIs").html("").html("企业选择不公示");
                }
                //净利润是否公示
                var netIncDisIs = data.data[0].netIncDis;
                if (netIncDisIs == 1) {
                    $("#netIncDisIs").html("").html(data.data[0].netInc+" 万元");
                } else {
                    $("#netIncDisIs").html("").html("企业选择不公示");
                }
                //纳税总额是否公示
                var ratGroDisIs = data.data[0].ratGroDis;
                if (ratGroDisIs == 1) {
                    $("#ratGroDisIs").html("").html(data.data[0].ratGro+" 万元");
                } else {
                    $("#ratGroDisIs").html("").html("企业选择不公示");
                }
                //负债总额是否公示
                var liaGroDisIs = data.data[0].liaGroDis;
                if (liaGroDisIs == 1) {
                    $("#liaGroDisIs").html("").html(data.data[0].liaGro+" 万元");
                } else {
                    $("#liaGroDisIs").html("").html("企业选择不公示");
                }
            }
        }
    })
    //个体年报信息   生产经营状况信息  是否公示
    $.ajax({
        type: "POST",
        url: vAnnPbAssetUrl,
        dataType: "JSON",
        async: false,
        success: function (data) {
            if (data.data != null && data.data.length > 0) {
                //纳税总额是否公示
                var ratGroDisIs = data.data[0].ratGroDis;
                if (ratGroDisIs == 1) {
                    $("#ratGroDis").html("").html(data.data[0].ratGro);
                } else {
                    $("#ratGroDis").html("").html("个体户选择不公示");
                }
                //营业额是否公示
                var vendIncDisIs = data.data[0].vendIncDis;
                if (vendIncDisIs == 1) {
                    $("#vendIncDis").html("").html(data.data[0].vendInc);
                } else {
                    $("#vendIncDis").html("").html("个体户选择不公示");
                }
            }else{
                $('#needPaging_AnRepAsset').remove();
                $('#one').append('<p>暂无企业资产状况信息</p>');
            }
        }
    })
    //农专年报信息   生产经营状况信息  是否公示
    $.ajax({
        type: "POST",
        url: vannualSfcAssertUrl,
        dataType: "JSON",
        async: false,
        success: function (data) {
            if (data.data != null && data.data.length > 0) {
                //销售额或营业收入是否公示
                var priYeaSalesDisIs = data.data[0].priYeaSalesDis;
                if (priYeaSalesDisIs == 1) {
                    $("#priYeaSales").html("").html(data.data[0].priYeaSales);
                } else {
                    $("#priYeaSales").html("").html("农专社选择不公示");
                }
                //盈余总额是否公示
                var priYeaProfitDisIs = data.data[0].priYeaProfitDis;
                if (priYeaProfitDisIs == 1) {
                    $("#priYeaProfit").html("").html(data.data[0].priYeaProfit);
                } else {
                    $("#priYeaProfit").html("").html("农专社选择不公示");
                }
                //获得政府扶持资金、补助是否公示
                var priYeaSubDisIs = data.data[0].priYeaSubDis;
                if (priYeaSubDisIs == 1) {
                    $("#priYeaSub").html("").html(data.data[0].priYeaSub);
                } else {
                    $("#priYeaSub").html("").html("农专社选择不公示");
                }
                //纳税金额是否公示
                var ratGroDisIs = data.data[0].ratGroDis;
                if (ratGroDisIs == 1) {
                    $("#ratGro").html("").html(data.data[0].ratGro);
                } else {
                    $("#ratGro").html("").html("农专社选择不公示");
                }
                //金融贷款是否公示
                var priYeaLoanDisIs = data.data[0].priYeaLoanDis;
                if (priYeaLoanDisIs == 1) {
                    $("#priYeaLoan").html("").html(data.data[0].priYeaLoan);
                } else {
                    $("#priYeaLoan").html("").html("农专社选择不公示");
                }
            }
            // else{//暂时去除无数据提醒
            //     $('#needPaging_sfc_assert').remove();
            //     $('#oneSfcAss').append('<p>暂无企业资产状况信息</p>');
            // }

        }
    })

    //获取企业类型信息
    var ent_type_result = document.getElementById("entType").value;


    // 年报年份下拉选择框
    $(".chooseAnnual").bind("click", function () {
        $(".drop-down-years").slideToggle();
    });


    // 企业年报信息
    //企业年报中的基本信息
    var VAnnualReportBaseinfoColumns = [
        "uniscId",
        "entName",
        "tel",
        "addr",
        "postalCode",
        "email",
        "busSt_CN",
        "empNum"
    ];
    //网站信息
