	function pledgeDetail(index){
	  row = pledgeList[index];
	  console.log(row);
	  if (row.vStakQualitInfoAlt.length > 0) {
			$("#pledge_cancel_div").hide();
			$("#pledge_revoke_div").hide();
			$("#pledge_alt_div").show();
			$("#pledge_alter_table tbody").empty();
			for (var i=0;i<row.vStakQualitInfoAlt.length;i++) {
				var indexTemp = parseInt(i) + 1;
				$("#pledge_alter_table tbody").append("<tr><td>" + indexTemp + "</td><td>" + convertDate(row.vStakQualitInfoAlt[i].altDate) + "</td><td>" + row.vStakQualitInfoAlt[i].alt + "</td></tr>");
			}
      }
      if (row.type == "2") {
          var isCancel = false;
          if (row.cancelDate != null || (row.cancelRea != null && row.cancelRea != '')) {
        	  $("#pledge_cancel_div").hide();
  			  $("#pledge_alt_div").hide();
  			  $("#pledge_revoke_div").show();
  			  $("#pledge_revoke_date").html(convertDate(row.cancelDate));
  			  $("#pledge_revoke_rea").html(row.cancelRea);
          }
          if (row.canDate != null || (row.equpleCanRea != null && row.equpleCanRea != '')) {
        	  $("#pledge_revoke_div").hide();
  			  $("#pledge_alt_div").hide();
  			  $("#pledge_cancel_div").show();
  			  $("#pledge_cancel_date").html(convertDate(row.canDate));
			  $("#pledge_cancel_rea").html(row.equPleCanRea);
          }
        } else {
        	$("#pledge_cancel_div").hide();
			$("#pledge_revoke_div").hide();
			$("#pledge_alt_div").show();
			$("#pledge_alter_table tbody").empty();
			$("#pledge_alter_table tbody").append("<tr><td colspan='3'>暂无股权出质变更信息</td></tr>");
        }
	    document.getElementById("bg").style.display ="block";
	    document.getElementById("pledge_alter").style.display ="block";
	}
	
	function mortRegDetail(morregId,morRegCNo,regiDate,regOrgCN){
			var regiDateF = convertDate(Number(regiDate));
