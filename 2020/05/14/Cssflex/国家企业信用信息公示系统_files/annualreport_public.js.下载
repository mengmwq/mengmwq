                            $("#annualAssetsColor3").css("color", "#337bb8");
                            $("#annualToOutColor").css("color", "#337bb8");
                            $("#annualSponColor").css("color", "#337bb8");
                            $("#annualWebColor").css("color", "#337bb8");
                            $("#annualBaseInfoColor1").css("color", "#337bb8");
                            $("#annualBaseInfoColor2").css("color", "#337bb8");
                            $("#annualBaseInfoColor3").css("color", "#337bb8");
                            $("#annualBaseInfoColor4").css("color", "#337bb8");
                            $("#addrForColor1").css("color", "#131313");
                            $("#addrForColor2").css("color", "#131313");
                            $("#uniscIdForColor1").css("color", "#131313");
                            $("#uniscIdForColor2").css("color", "#131313");
                            $("#uniscIdForColor3").css("color", "#131313");
                            $("#uniscIdForColor4").css("color", "#131313");
                            $("#entNameForColor1").css("color", "#131313");
                            $("#entNameForColor2").css("color", "#131313");
                            $("#entNameForColor3").css("color", "#131313");
                            $("#entNameForColor4").css("color", "#131313");
                            $("#telForColor1").css("color", "#131313");
                            $("#telForColor2").css("color", "#131313");
                            $("#telForColor3").css("color", "#131313");
                            $("#telForColor4").css("color", "#131313");
                            $("#postForColor1").css("color", "#131313");
                            $("#postForColor2").css("color", "#131313");
                            $("#emailForColor1").css("color", "#131313");
                            $("#emailForColor2").css("color", "#131313");
                            $("#emailForColor3").css("color", "#131313");
                            $("#empNumForColor1").css("color", "#131313");
                            $("#empNumForColor2").css("color", "#131313");
                            $("#empNumForColor3").css("color", "#131313");
                            $("#empNumForColor4").css("color", "#131313");
                            $("#busStForColor1").css("color", "#131313");
                            $("#busStForColor2").css("color", "#131313");
                            $("#dependForColor2").css("color", "#131313");
                            $("#SfcfarnumForColor3").css("color", "#131313");
                            $("#SfcannNewMemNumForColor3").css("color", "#131313");
                            $("#SfcannRedMemNumForColor3").css("color", "#131313");
                            $("#pbNameForColor4").css("color", "#131313");
                            $("#PbfundAmForColor4").css("color", "#131313");
                        }

                        return "<tr>" + row.alitem + "</tr>";
                    }
                }, {
                    "targets": [0],
                    "render": function (data, x, obj, index, y) {
                        return index.row + index.settings._iDisplayStart + 1;
                    }
                }]
        });
    }


    //默认网店信息为  否
    $("#webIsOrNot").html("否");

    //网站网店信息数据
    var dataForWeb = null;
    //年报中网站网店信息  全部信息   格子形式网店网站信息
    if (webSiteInfoUrl != null) {
        $.ajax({
            url: webSiteInfoUrl + "?entType=" + ent_type_result,
            async: false,
            success: function (data) {
                if (data.totalPage > 1) {
                    $("#annualWebAll").append("<a href='" + allWebInfoModelUrl + "?entType=" + ent_type_result + "' target='_blank' style='float:right;color:#337bb8;'>查看全部 &gt;&gt;</a>");
                } else {
                    $("#annualWebAll").append("");
                }
                if (data != null && data.data.length > 0) {
                    dataForWeb = data;
                    if (data != null) {
                        $("#webIsOrNot").html("是");
                    }
                    //格子形式网站网店信息
                    if (data != null && data.data.length > 0) {
                        for (var i = 0; i < data.data.length; i++) {
                            var strForWeb = "";
                            if (data.data[i].webSitName != null && data.data[i].webSitName != "" && data.data[i].webSitName.length > 0) {
                                if (data.data[i].webSitName.length > 14) {
                                    strForWeb = data.data[i].webSitName.substring(0, 14);
                                    strForWeb += "...";
                                } else {
                                    strForWeb = data.data[i].webSitName;
                                }
                            }
                            var strForDomain = "";
                            if (data.data[i].domain != null && data.data[i].domain != "" && data.data[i].domain.length > 0) {
                                if (data.data[i].webSitName.length > 18) {
                                    strForDomain = data.data[i].domain.substring(0, 18);
                                    strForDomain += "...";
                                } else {
                                    strForDomain = data.data[i].domain;
                                }
                            }
                            $("#webSiteList").append('<li><a><div style="font-size:14px;color:#333333;" title="' + data.data[i].webSitName + '">' + strForWeb +
                                '</div><span style="font-size:12px;color:#666666;">· 类型 : '
                                + '<span style="font-size:12px;color:#333333;" title=' + (data.data[i].webType == 1 ? "网站" : "网店") + '>' + (data.data[i].webType == 1 ? "网站" : "网店") + '</span>' +
                                '</span><div style="font-size:12px;color:#666666;position:relative;top:-10px">· 网址 : ' +
                                '<span style="font-size:12px;color:#333333;" title="' + data.data[i].domain + '">' + data.data[i].domain + '</span></div></a></li>');
                        }
                    }
                } else {
                    $("#webSiteList").html("").html("<tr class='odd'><td  style='background:#F3F7FA;color:#666;width:1077px;' id='dataTables_empty' colspan='4'>暂无网站或网店信息</td></tr>");
                }

            }
        })
    }

    //初始化  对外投资信息 变量
    var forinvestmentforhome = null;

    //默认 为否
    $("#forInvestment").html("否");

    //年报中对外投资信息  全部信息
    if (forInvestmentUrl != null) {
        $.ajax({
            url: forInvestmentUrl,
            async: false,
            success: function (data) {
                if (data.totalPage > 1) {
                    $("#annualToOutAll").append("<a href='" + allForgModelUrl + "' target='_blank' style='float:right;color:#337bb8;'>查看全部 &gt;&gt;</a>");
                } else {
                    $("#annualToOutAll").append("");
                }
                forinvestmentforhome = data.data.length;
                if (forinvestmentforhome) {
                    $("#forInvestment").html("是");
                }
                //格子形式  对外投资信息
                if (data != null && data.data.length > 0) {
                    for (var i = 0; i < data.data.length; i++) {
                        var strForEntName = "";
                        if (data.data[i].entName != "" && data.data[i].entName != null && data.data[i].entName.length > 0) {
                            if (data.data[i].entName.length > 22) {
                                strForEntName = data.data[i].entName.substring(0, 22);
                                strForEntName += "...";
                            } else {
                                strForEntName = data.data[i].entName;
                            }
                        }

                        var strForUnscId = "";
                        if (data.data[i].uniscId != "" && data.data[i].uniscId != null && data.data[i].uniscId.length > 0) {                          
                        	strForUnscId = data.data[i].uniscId;
                        }

                        $("#forInvsmentList").append('<li><a><div style="font-size:14px;color:#333333;" title=' + data.data[i].entName + '>'
                            + '' + strForEntName +
                            '</div><span style="font-size:12px;color:#666666;">· 统一社会信用代码/注册号 : '
                            + '<span style="font-size:12px;color:#333333;" title=' + data.data[i].uniscId + '>' + strForUnscId + '</span></span></a></li>');
                    }
                } else {
                    $("#forInvsmentList").html("").html("<tr class='odd'><td  style='background:#F3F7FA;color:#666;width:1077px;' id='dataTables_empty' colspan='4'>暂无对外投资信息</td></tr>");
                }
            }
        })
    }

}

