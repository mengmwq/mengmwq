function forAnnualDetail(entTypeJs){
	var entTypeResultForJs1 = document.getElementById("entType").value;
	//网店网址
						$.ajax({
									type : "POST",
									url : anCheYearInfo,
									dataType : "JSON",
									async : false,
									success : function(data) {
										// 判断是否为null
										if (data.length === 0) {
											$("#reportName").prepend(
													"该企业没有报送的年报信息年度报告");
											$("span#Annual_modify").css(
													"display", "none");
											$("span#reportFillIn").prepend("无");
											$("#annualReportHidden").css(
													"display", "none");
										} else {
											// 取json list数据
											
											var json = eval(data);
											for (var i = 0; i < json.length; i++) {
												var anCheIdJs1 = json[i].anCheId;
												var entTypeJs1 = json[i].entType;
												var anCheYearJs1 = json[i].anCheYear;

												// 条件判断entType
												if ((entTypeResultForJs1> 0 && entTypeResultForJs1< 19)
														&& (entTypeResultForJs1 != 18)
														&& entTypeResultForJs1 == 16) {
													if (entTypeJs == "pb") {
														$("#AnCheYearAnnual")
																.append(
																		"<option class ='year'"
																		+ " value="+anCheIdJs1+","+entTypeJs+","+anCheYearJs1+","+entTypeResultForJs1+">"
																		+ anCheYearJs1
																		+ "</option>");
													}
												} else if ((entTypeResultForJs1 > 0 && entTypeResultForJs1 < 19)
														&& (entTypeResultForJs1 != 18)
														&& entTypeResultForJs1 == 17) {
													if (entTypeJs == "sfc") {
														$("#AnCheYearAnnual")
																.append(
																		"<option class ='year'"
																		+ " value="+anCheIdJs1+","+entTypeJs+","+anCheYearJs1+","+entTypeResultForJs1+">"
																		+ anCheYearJs1
																		+ "</option>");
													}
													
												} else if ((entTypeResultForJs1 > 0 && entTypeResultForJs1 < 19)
														&& (entTypeResultForJs1 != 18)) {
													if (entTypeJs == "e") {
														$("#AnCheYearAnnual")
																.append(
																		"<option class ='year'"
																		+ " value="+anCheIdJs1+","+entTypeJs+","+anCheYearJs1+","+entTypeResultForJs1+">"
																		+ anCheYearJs1
																		+ "</option>");
													}
												}
												

											}

													
										}
									}
								});
}