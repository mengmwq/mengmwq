			$("#mortgage_alter_regiDate").html(regiDateF);
			$("#mortgage_alter_RegOrg_CN").html(regOrgCN);

			
		$.ajax({
			type:"GET",
			url:"/corp-query-entprise-info-mortregpersoninfo-"+morregId+".html",
			dataType:"JSON",
			async: false,
			success : function(data){
				var id = null;
				var num = 0;
				if (data.data.length == 0){
					$("#person").append("<tr class='temp'><td colspan = '5'>暂无抵押权人信息</td></tr> ");
				}else{
					for(var i=0;i<data.data.length;i++){
						if(data.data[i].morReg_Id == morregId){
							id++;
							$("#person").append("<tr class='temp'><td class='entry'>"+id+"</td><td class='entry'>"+data.data[i].more+"</td><td class='entry'>"+data.data[i].bLicType_CN+"</td><td class='entry'>"+data.data[i].bLicNo+"</td><td class='entry'>"+data.data[i].morLoc+"</td></tr> ");
						} else {
							$("#person").append("<tr class='temp'><td colspan = '5'>暂无抵押权人信息</td></tr> ");
						}
					}
				}	
			}
		})
		
		$.ajax({
			type:"GET",
			url:"/corp-query-entprise-info-mortCreditorRightInfo-"+morregId+".html",
			dataType:"JSON",
			async: false,
			success : function(data){
				var num =0;
				if (data.data.length == 0){
					$("#priClaSec").hide();
