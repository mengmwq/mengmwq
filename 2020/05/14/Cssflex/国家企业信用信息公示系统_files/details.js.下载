							/*$("#mortgage_alter_pefPerTo").html("").html("暂无被担保主债权信息");*/
							$("#mortgage_alter_cremark").html("").html("暂无被担保主债权信息")
						}
					}
				}
			}
		})
		
		$.ajax({
			type:"GET",
			url:"/corp-query-entprise-info-mortGuaranteeInfo-"+morregId+".html",
			dataType:"JSON",
			async: false,
			success : function(data){
				var num =0;
				var mortGuaranteeInfoId = null;
				if (data.data.length == 0){
					$("#mortGuaranteeInfo").append("<tr class='temp'><td colspan = '5'>暂无抵押物信息</td></tr> ");
				} else {
					for(var i=0;i<data.data.length;i++){
						if(data.data[i].morReg_Id == morregId){
							mortGuaranteeInfoId++;
							$("#mortGuaranteeInfo").append("<tr class='temp'><td class='entry'>"+mortGuaranteeInfoId+"</td><td class='entry'>"+data.data[i].guaName+"</td><td class='entry'>"+data.data[i].own+"</td><td class='entry'>"+data.data[i].guaDes+"</td><td class='entry'>"+data.data[i].remark+"</td></tr> ");
						} else {
							$("#mortGuaranteeInfo").append("<tr class='temp'><td colspan = '5'>暂无抵押物信息</td></tr> ");
						}
					}
				}
			}
		})
		
		$.ajax({
			type:"GET",
			url:"/corp-query-entprise-info-getMortAltItemInfo-"+morregId+".html",
			dataType:"JSON",
			async: false,
			success : function(data){
				var num =0;
				var mortAltItemInfoId = null;
				if (data.data.length == 0){
					$("#alt").append("<tr class='temp'><td colspan = '5'>暂无变更信息</td></tr> ");
				} else {
					for(var i=0;i<data.data.length;i++){
						if(data.data[i].morReg_Id == morregId){
							mortAltItemInfoId++;
							if(data.data[i].altDate == null){
								var altDate = "";
							}else{
								var altDate = convertDate(data.data[i].altDate);
							}
							$("#alt").append("<tr class='temp'><td class='entry'>"+mortAltItemInfoId+"</td><td class='entry'>"+altDate+"</td><td class='entry'>"+data.data[i].alt+"</td></tr> ");
						} else {
							$("#alt").append("<tr class='temp'><td colspan = '5'>暂无变更信息</td></tr> ");
						}
					}
				}
			}
		})
		
		$.ajax({
			type:"GET",
			url:"/corp-query-entprise-info-getMortRegCancelInfo-"+morregId+".html",
			dataType:"JSON",
			async: false,
			success : function(data){
				var num =0;
				if (data.data.length == 0){
					$("#cancel").hide();
					$("#cancel2").show();
					$("#cancel2").append("<tr class='temp'><td colspan = '5'>暂无注销信息</td></tr> ");
				} else {
					$("#cancel2").hide();
					$("#cancel").show();
					for(var i=0;i<data.data.length;i++){
						if(data.data[i].morReg_Id == morregId){
							var canDate = convertDate(data.data[i].canDate);
						//	$("#cancel").append("<tr class='temp'><td class='entry'>"+canDate+"</td><td class='entry'>"+data.data[i].morCanRea_CN+"</td></tr> ");
							$("#mortgage_alter_canDate").html("").html(canDate);
							$("#mortgage_alter_morCanRea_CN").html("").html(data.data[i].morCanRea_CN);
						} else {
						//	$("#cancel").append("<tr class='temp'><td colspan = '5'>暂无注销信息</td></tr> ");
							$("#mortgage_alter_canDate").html("").html("暂无注销信息");
							$("#mortgage_alter_morCanRea_CN").html("").html("暂无注销信息");
						}
					}
				}
			}
		})
		//}
		//$("#mortgage_alter").show();
        document.getElementById("bg").style.display ="block";
        document.getElementById("mortgage_alter").style.display ="block";
	}
	
	
