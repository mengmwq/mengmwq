		//企业资产信息是否公示
		$.ajax({
			type : "POST",
			url : baseinfoUrl,
			dataType : "JSON",
			async : false,
			success : function(data) {
				if(data.data != null && data.data.length > 0){
					//资产总额是否公示
					var assGroDisIs = data.data[0].assGroDis;
					if(assGroDisIs == 1){
						$("#assGroDisIs").html("").html(data.data[0].assGro);
					}else{
						$("#assGroDisIs").html("").html("企业选择不公示");
					}
					//所有者权益合计是否公示
					var totEquDisIs =  data.data[0].totEquDis;
					if(totEquDisIs==1){
						$("#totEquDisIs").html("").html(data.data[0].totEqu);
					}else{
						$("#totEquDisIs").html("").html("企业选择不公示");
					}
					//营业总收入是否公示
					var vendIncDisIs = data.data[0].vendIncDis;
					if(vendIncDisIs==1){
						$("#vendIncDisIs").html("").html(data.data[0].vendInc);
					}else{
						$("#vendIncDisIs").html("").html("企业选择不公示");
					}
					//利润总额是否公示
					var proGroDisIs = data.data[0].proGroDis;
					if(proGroDisIs==1){
						$("#proGroDisIs").html("").html(data.data[0].proGro);
					}else{
						$("#proGroDisIs").html("").html("企业选择不公示");
					}
					//其中主营业务收入是否公示
