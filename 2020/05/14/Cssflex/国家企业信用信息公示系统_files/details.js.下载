	var insLicenceDetailInfoUrl = null;
	//企业即时信息的详情
	//行政许可信息
	function insLicenceDetail(status,licId,canDate,equPleCanRea,revDate,sugRevReason,invalidDate,invalidRea){
		//往后台传数据的

		$.ajax({
			type:"GET",
			url:insLicenceInfoForJsUrl+licId+".html",
			dataType:"JSON",
			async: false,
			success : function(data){
				var json =  eval(data);
				insLicenceDetailInfoUrl = json.insLicenceDetailInfoUrl;
				$.ajax({
					type:"GET",
					url:insLicenceDetailInfoUrl,
					dataType:"JSON",
					async: false,
					success : function(data){
							if(status == 1){
								//控制页面显示与隐藏
								$("table#ins_detail").show();
								$("table#ins_detail_invalidLogout").hide();
								$("table#ins_detail_invalidRevocation").hide();
								$("table#ins_detail_invalidOtherRes").hide();
								$("div#alert_Info1").show();
								$("div#conDate1").hide();
								$("div#offDate1").hide();
								$("div#noUseDate1").hide();
								var str ="" +
										"<tr><th class='title'>变更事项</th><th class='title'>变更前内容" +
										"</th><th class='title'>变更后内容</th><th class='title'>变更日期</th></tr>";
								for(var i =0;i<data.list.length;i++){
									var javascriptDate = convertDate(data.list[i].altDate);
									str =str + "<tr><td>"+data.list[i].alt+"</td>"+"<td>"+data.list[i].altBe+"</td>"+
									"<td>"+data.list[i].altAf+"</td>"+"<td>"+javascriptDate+"</td></tr>";
								}
								$("#ins_detail").html(str);
							}
							else{
								//注销原因
								if(canDate != null && canDate != 'null' && typeof(canDate) != 'undefined'){
									$("table#ins_detail").hide();
									$("table#ins_detail_invalidLogout").show();
									$("table#ins_detail_invalidRevocation").hide();
									$("table#ins_detail_invalidOtherRes").hide();
									$("div#alert_Info1").hide();
									$("div#conDate1").show();
									$("div#offDate1").hide();
									$("div#noUseDate1").hide();
									if(canDate == "null"){
										$("div#conDate1").css('opacity', 0);
										$("table#ins_detail_invalidLogout").html("<p class='txtStyle'>暂无无效信息</p>");
									}else{
										var mydate = parseInt(canDate);
										var javascriptDate = convertDate(mydate);
										$("#ins_licence_detail_EquDate").html("").html(javascriptDate);
										$("#ins_licence_detail_EquRes").html("").html(equPleCanRea);
									}
								}
								//吊销原因
								else if(revDate != null && revDate != 'null' && typeof(revDate) != 'undefined'){
									$("table#ins_detail").hide();
									$("table#ins_detail_invalidLogout").hide();
									$("table#ins_detail_invalidRevocation").show();
									$("table#ins_detail_invalidOtherRes").hide();
									$("div#alert_Info1").hide();
									$("div#conDate1").hide();
									$("div#offDate1").show();
									$("div#noUseDate1").hide();
									if(revDate == "null"){
										$("div#conDate1").css('opacity', 0);
										$("table#ins_detail_invalidRevocation").html("<p class='txtStyle'>暂无无效信息</p>");
										// $("#ins_licence_detail_SugDate").html("").html("");
										// $("#ins_licence_detail_SugRes").html("").html(sugRevReason);
									}else{
										var mydate = parseInt(revDate);
										var javascriptDate = convertDate(mydate);
										$("#ins_licence_detail_SugDate").html("").html(javascriptDate);
										$("#ins_licence_detail_SugRes").html("").html(sugRevReason);
									}
								}
								//其他原因
								else if(invalidDate !=null && invalidDate != 'null' && typeof(invalidDate) != 'undefined'){
									$("table#ins_detail").hide();
									$("table#ins_detail_invalidLogout").hide();
									$("table#ins_detail_invalidRevocation").hide();
									$("table#ins_detail_invalidOtherRes").show();
									$("div#alert_Info1").hide();
									$("div#conDate1").hide();
									$("div#offDate1").hide();
									$("div#noUseDate1").show();
									if(invalidDate == "null"){
										$("div#conDate1").css('opacity', 0);
										$("table#ins_detail_invalidOtherRes").html("<p class='txtStyle'>暂无无效信息</p>");
										// $("#ins_licence_detail_InvDate").html("").html("");
										// $("#ins_licence_detail_InvRes").html("").html(invalidRea);
									}else{
										var mydate = parseInt(invalidDate);
										var javascriptDate = convertDate(mydate);
										$("#ins_licence_detail_InvDate").html("").html(javascriptDate);
										$("#ins_licence_detail_InvRes").html("").html(invalidRea);
									}
								}
								
							}
					}
				})
			}
		})
		//$(".ins_licence_detail").show();
        document.getElementById("bg").style.display ="block";
        document.getElementById("ins_licence_detail").style.display ="block";
	}
	var insImKlpDetailInfoUrl = null;
	//知识产权出质信息
	function insIntellectualDetail(pleId,type,canDate,equPleCanRea,invalidDate,invalidRea){
		//往后台传数据的
		$.ajax({
			type:"GET",
			url:insImKlpInfoForJsUrl+pleId+".html",
			dataType:"JSON",
			async: false,
			success : function(data){
				var json =  eval(data);
				insImKlpDetailInfoUrl = json.insImKlpDetailInfoUrl;
				$.ajax({
					type:"GET",
					url:insImKlpDetailInfoUrl,
					dataType:"JSON",
					async: false,
					success : function(data){
							if(type == 1){
								//控制页面显示与隐藏
								$("table#ins_ImKlp_detailInfo").show();
								$("table#ins_ImKlp_detailInfoLayout").hide();
								$("table#ins_ImKlp_detailInfoOther").hide();
								$("div#knowlage1").show();
								$("div#knowlageConDate1").hide();
								$("div#knowlageNoUse1").hide();						
								var str ="<tr><th class='title'>序号</th><th class='title'>变更事项</th><th class='title'>变更时间</th>" +
										"<th class='title'>变更前内容</th><th class='title'>变更后内容</th></tr>";
								
								for(var i =0;i<data.list.length;i++){
									var javascriptDate = convertDate(data.list[i].altDate);
									str =str +"<tr><td>"+(i+1)+"</td><td>"+data.list[i].alt+"</td><td>"+javascriptDate+""+"</td><td>"+data.list[i].altBe+"</td><td>"+data.list[i].altAf +
									"</td></tr>";
								}
								$("#ins_ImKlp_detailInfo").html(str);
							}
							else{
								//注销原因
								if(type == 2){
									$("table#ins_ImKlp_detailInfo").hide();
									$("table#ins_ImKlp_detailInfoLayout").show();
									$("table#ins_ImKlp_detailInfoOther").hide();
									$("div#knowlage1").hide();
									$("div#knowlageConDate1").show();
									$("div#knowlageNoUse1").hide();
									var mydate = parseInt(canDate);
									if(canDate == "null"){
										$("#ins_ImKlp_detail_EquDate").html("").html("");
										$("#ins_ImKlp_detail_EquRes").html("").html(equPleCanRea);
									}else{
										var javascriptDate = convertDate(mydate);
										$("#ins_ImKlp_detail_EquDate").html("").html(javascriptDate);
										$("#ins_ImKlp_detail_EquRes").html("").html(equPleCanRea);
									}
								}
								//其他原因
								else{
									$("table#ins_ImKlp_detailInfo").hide();
									$("table#ins_ImKlp_detailInfoLayout").hide();
									$("table#ins_ImKlp_detailInfoOther").show();
									$("div#knowlage1").hide();
									$("div#knowlageConDate1").hide();
									$("div#knowlageNoUse1").show();
									var mydate = parseInt(invalidDate);
									if(invalidDate == "null"){
										$("#ins_ImKlp_detail_InvDate").html("").html("");
										$("#ins_ImKlp_detail_InvRes").html("").html(invalidRea);
									}else{
										var javascriptDate = convertDate(mydate);
										$("#ins_ImKlp_detail_InvDate").html("").html(javascriptDate);
										$("#ins_ImKlp_detail_InvRes").html("").html(invalidRea);
									}
								}
								
							}
					}
				})
			}
		})
		//$(".ins_intellectual_detail").show();
        document.getElementById("bg").style.display ="block";
        document.getElementById("ins_intellectual_detail").style.display ="block";
	}
	var insPunshmentDetailInfoUrl = null;
	//行政处罚信息 
	function insSanctionDetail(caseId){
		$.ajax({
			type:"GET",
			url:insPunshmentInfoForJsUrl+caseId+".html",
			dataType:"JSON",
			async: false,
			success : function(data){
				var json =  eval(data);
				insPunshmentDetailInfoUrl = json.insPunshmentDetailInfoUrl;

				$.ajax({
					type:"GET",
					url:insPunshmentDetailInfoUrl,
					dataType:"JSON",
					async: false,
					success : function(data){
							if(data.list.length == 0 ){
								//控制页面显示与隐藏
								$("table#ins_sanction_detailInfo").show();
								var str ="<tr><th class='title'>变更日期</th>" +
														"<th class='title'>作出变更决定机关名称</th>" +
																"<th class='title'>变更内容</th>" +
																		"<th class='title'>备注</th></tr>"+"<tr><td class='title textcenter' colspan='4'>无信息</td></tr>";
								$("#ins_sanction_detailInfo").html(str);
							}else{
								
								$("table#ins_sanction_detailInfo").show();
								var str ="<tr><th class='title'>变更日期</th>" +
														"<th class='title'>作出变更决定机关名称</th>" +
																"<th class='title'>变更内容</th>" +
																		"<th class='title'>备注</th></tr>";
								for(var i =0;i<data.list.length;i++){
									var javascriptDate = null;
									if(data.list[i].altDate == null){
										javascriptDate = "";
									}else{
										var javascriptDate = convertDate(data.list[i].altDate);
									}
									str = str +"<tr><td>"+javascriptDate+"</td><td>"+data.list[i].penAuth+"</td><td>"+data.list[i].alt+"</td><td>"+data.list[i].remark+"</td></tr>"
								}
								$("#ins_sanction_detailInfo").html(str);
							}
					}
				})
			}
		})
        document.getElementById("bg").style.display ="block";
        document.getElementById("ins_sanction_detail").style.display ="block";
	}
	
	
	
