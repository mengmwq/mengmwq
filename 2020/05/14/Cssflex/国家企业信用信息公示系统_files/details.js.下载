	function punishmentDetail(punishmentDetailByCaseIdUrl,pripId){
		if(true){
		$.ajax({
			type:"POST",
			//url:"/corp-query-entprise-info-punishmentDetailInfoByCaseId-"+caseId+".html",
			url:punishmentDetailByCaseIdUrl,
			async: true,
			success : function(data){
				$("#punish_detail_penDecNo").html("");
				$("#punish_detail_name").html("");
				$("#punish_detail_regNo").html("");
				$("#punish_detail_leRep").html("");
				$('#punish_detail_illegActType').html("");
				$("#punish_detail_penContent").html("");
				$("#punish_detail_penAuth_CN").html("");
				$("#punish_detail_PenDecIssDate").html("");
				      var punishmentInfo;
				      if(data!=null && data.length>0){
				    	  for(var i=0;i<data.length;i++){
				    		  if(pripId==data[i].pripId){
				    			  punishmentInfo = data[i];
				    		  }
				    	  }
				      }
                        if(punishmentInfo != null){
		                    	$("#punish_detail_penDecNo").html(punishmentInfo.penDecNo);
		  						var entType = $("#entType").val();
		  						if (entType == 16) {
		  							$("#punish_detail_name").html(punishmentInfo.name);
		  							$("#punish_detail_regNo").html(punishmentInfo.gtRegNo);
		  						} else {
		  							if (punishmentInfo.uniscId == null || punishmentInfo.uniscId == "") {
		  								$("#punish_detail_regNo").html(punishmentInfo.regNo);
		  							} else {
		  								$("#punish_detail_regNo").html(punishmentInfo.uniscId);
		  							}
		  							$("#punish_detail_name").html(punishmentInfo.unitName);
		  						}
		  						$("#punish_detail_leRep").html(punishmentInfo.leRep);
		  						$('#punish_detail_illegActType').html(punishmentInfo.illegActType);
		  						$("#punish_detail_penContent").html(punishmentInfo.penContent);
		  						$("#punish_detail_penAuth_CN").html(punishmentInfo.penAuth_CN);
		  						var penDecIssDate = convertDate(punishmentInfo.penDecIssDate);
		  						$("#punish_detail_PenDecIssDate").html(penDecIssDate);
                       }
					}
		     })
		}
		
        document.getElementById("bg").style.display ="block";
        document.getElementById("punish_detail").style.display ="block";
	}
	
	//司法股权冻结信息中的状态为“解除冻结”
	function freezeDetail(parent_Id){
		console.log("/corp-query-entprise-info-judiciaryStockfreeze-" + parent_Id + ".html");
		$.ajax({
			type:"GET",
			url:"/corp-query-entprise-info-judiciaryStockfreeze-" + parent_Id + ".html",
			dataType:"JSON",
			async: true,
			success : function(data){
				document.getElementById("baseDetailCancel").style.display = ""
				document.getElementById("continuation").style.display = ""
				document.getElementById("freezeCancelInfo").style.display = ""
				document.getElementById("invalid").style.display = ""
				document.getElementById("one").style.display = ""
				document.getElementById("two").style.display = ""
				document.getElementById("three").style.display = ""
				document.getElementById("four").style.display = ""
				var z = 0;
				var j = 0;
				var k = 0;
				var g = 0;
				$("#baseDetailCancel [class='temp']").remove();
				$("#continuation [class='temp']").remove();
				$("#freezeCancelInfo [class='temp']").remove();
				$("#invalid [class='temp']").remove();
				for(var i=0;i<data.data.length;i++){
					for(var s in data.data[i]){
						if(!$.trim(data.data[i][s])) data.data[i][s] = '';
					}
					
						if(data.data[i].frozState == "1"){
							if(data.data[i].keepFrozDeadline != "" && data.data[i].keepFrozDeadline > 0){
								var froFrom = convertDate(data.data[i].keepFroFrom);
								var froTo = convertDate(data.data[i].keepFroTo);
								var publicDate = convertDate(data.data[i].publicDate);
								$("#froAuth").html("").html("<th class='title'>执行法院</th><td class='entry'>"+data.data[i].froAuth+"</td><th class='title'>执行事项</th><td class='entry'>"+data.data[i].executeItem_CN+"</td>");
								$("#froDocNo").html("").html("<th class='title'>执行裁定书文号</th><td class='entry'>"+data.data[i].froDocNo+"</td><th class='title'>执行通知书文号</th><td class='entry'>"+data.data[i].executeNo+"</td>");
								if(data.data[i].froAm==null||data.data[i].froAm==''){
									$("#inv").html("").html("<th class='title'>被执行人</th><td class='entry'>"+data.data[i].inv+"</td><th class='title'>被执行人持有股权、其它投资权益的数额</th><td class='entry'></td>");
								}else{
									$("#inv").html("").html("<th class='title'>被执行人</th><td class='entry'>"+data.data[i].inv+"</td><th class='title'>被执行人持有股权、其它投资权益的数额</th><td class='entry'>"+data.data[i].froAm+"万"+data.data[i].regCapCur_CN+"</td>");
								}
								
					            if(data.data[i].cerType_CN != ""){
					            	$("#cardType").html("").html("<th class='title'>被执行人证照种类</th><td class='entry'>"+data.data[i].cerType_CN+"</td> <th class='title'>被执行人证照号码</th><td class='entry'>"+""+"</td>");
					            }else if(data.data[i].cerType_CN == "" && data.data[i].bLicType_CN != ""){
					            	$("#cardType").html("").html("<th class='title'>被执行人证照种类</th><td class='entry'>"+data.data[i].bLicType_CN+"</td> <th class='title'>被执行人证照号码</th><td class='entry'>"+data.data[i].bLicNo+"</td>");
					            }else{
					            	$("#cardType").html("").html("<th class='title'>被执行人证照种类</th><td class='entry'>"+""+"</td> <th class='title'>被执行人证照号码</th><td class='entry'>"+""+"</td>");
					            }
								$("#dateFrom").html("").html(" <th class='title'>续行冻结期限自</th><td class='entry'>"+froFrom+"</td><th class='title'>续行冻结期限至</th><td class='entry'>"+froTo+"</td>");
					            $("#publicDate").html("").html(" <th class='title'>续行冻结期限</th><td class='entry'>"+data.data[i].keepFrozDeadline+"</td><th class='title'>公示日期</th><td class='entry'>"+publicDate+"</td>");
					            j++;
							}
							else{
								var froFrom = convertDate(data.data[i].froFrom);
								var froTo = convertDate(data.data[i].froTo);
								var publicDate = convertDate(data.data[i].publicDate);
								$("#froAuthAndThing").html("").html("<th class='title'>执行法院</th><td class='entry'>"+data.data[i].froAuth+"</td><th class='title'>执行事项</th><td class='entry'>"+data.data[i].executeItem_CN+"</td>");
					            $("#NumAndfroDocNo").html("").html("<th class='title'>执行裁定书文号</th><td class='entry'>"+data.data[i].froDocNo+"</td><th class='title'>执行通知书文号</th><td class='entry'>"+data.data[i].executeNo+"</td>");
					            if(data.data[i].froAm==null||data.data[i].froAm==''){
					            	$("#bePerson").html("").html("<th class='title'>被执行人</th><td class='entry'>"+data.data[i].inv+"</td><th class='title'>被执行人持有股权、其它投资权益的数额</th><td class='entry'></td>");
					            }else{
					            	$("#bePerson").html("").html("<th class='title'>被执行人</th><td class='entry'>"+data.data[i].inv+"</td><th class='title'>被执行人持有股权、其它投资权益的数额</th><td class='entry'>"+data.data[i].froAm+"万"+data.data[i].regCapCur_CN+"</td>");
					            }
					            
					            if(data.data[i].cerType_CN != ""){
					            	$("#cardTypeOne").html("").html("<th class='title'>被执行人证照种类</th><td class='entry'>"+data.data[i].cerType_CN+"</td> <th class='title'>被执行人证照号码</th><td class='entry'>"+""+"</td>");
					            }else if(data.data[i].cerType_CN == "" && data.data[i].bLicType_CN != ""){
					            	$("#cardTypeOne").html("").html("<th class='title'>被执行人证照种类</th><td class='entry'>"+data.data[i].bLicType_CN+"</td> <th class='title'>被执行人证照号码</th><td class='entry'>"+data.data[i].bLicNo+"</td>");
					            }else{
					            	$("#cardTypeOne").html("").html("<th class='title'>被执行人证照种类</th><td class='entry'>"+""+"</td> <th class='title'>被执行人证照号码</th><td class='entry'>"+""+"</td>");
					            }
					            $("#congeal").html("").html("<th class='title'>冻结期限自</th><td class='entry'>"+froFrom+"</td> <th class='title'>冻结期限至</th><td class='entry'>"+froTo+"</td>");
					            $("#congealAndDate").html("").html("<th class='title'>冻结期限</th><td class='entry'>"+data.data[i].frozDeadline+"</td><th class='title'>公示日期</th><td class='entry'>"+publicDate+"</td>");
								z++;
							} 
							
						}
						if(data.data[i].frozState == "2"){
							var thawDate = convertDate(data.data[i].thawDate);
							var publicDate = convertDate(data.data[i].publicDate);							
							$("#froAuthThree").html("").html("<th class='title'>执行法院</th><td class='entry'>"+data.data[i].froAuth+"</td><th class='title'>执行事项</th><td class='entry'>"+data.data[i].executeItem_CN+"</td>");
							$("#cardNumThree").html("").html("<th class='title'>执行裁定书文号</th><td class='entry'>"+data.data[i].froDocNo+"</td><th class='title'>执行通知书文号</th><td class='entry'>"+data.data[i].executeNo+"</td>");
							if(data.data[i].froAm==null||data.data[i].froAm==''){
								$("#personThree").html("").html("<th class='title'>被执行人</th><td class='entry'>"+data.data[i].inv+"</td><th class='title'>被执行人持有股权、其它投资权益的数额</th><td class='entry'></td>");
							}else{
								$("#personThree").html("").html("<th class='title'>被执行人</th><td class='entry'>"+data.data[i].inv+"</td><th class='title'>被执行人持有股权、其它投资权益的数额</th><td class='entry'>"+data.data[i].froAm+"万"+data.data[i].regCapCur_CN+"</td>");
							}
							
				            if(data.data[i].cerType_CN != ""){
				            	$("#cardTypeThree").html("").html("<th class='title'>被执行人证照种类</th><td class='entry'>"+data.data[i].cerType_CN+"</td> <th class='title'>被执行人证照号码</th><td class='entry'>"+""+"</td>");
				            }else if(data.data[i].cerType_CN == "" && data.data[i].bLicType_CN != ""){
				            	$("#cardTypeThree").html("").html("<th class='title'>被执行人证照种类</th><td class='entry'>"+data.data[i].bLicType_CN+"</td> <th class='title'>被执行人证照号码</th><td class='entry'>"+data.data[i].bLicNo+"</td>");
				            }else{
				            	$("#cardTypeThree").html("").html("<th class='title'>被执行人证照种类</th><td class='entry'>"+""+"</td> <th class='title'>被执行人证照号码</th><td class='entry'>"+""+"</td>");
				            }
							$("#conDateThree").html("").html("<th class='title'>解除冻结日期</th><td class='entry'>"+thawDate+"</td><th class='title'>公示日期</th><td class='entry'>"+publicDate+"</td>");
							k++;
						}
						if(data.data[i].frozState == "3"){
							var loseEffDate = convertDate(data.data[i].loseEffDate);
							$("#reson").html("").html("<th class='title'>失效原因</th><td class='entry'>"+data.data[i].loseEffRes_CN+"</td>");
							$("#time").html("").html("<th class='title'>失效时间</th><td class='entry'>"+loseEffDate+"</td>");
							g++;
						} 
					}
				if(z==0){
					document.getElementById("baseDetailCancel").style.display = "none"
						document.getElementById("one").style.display = "none"
				}
				if(j==0){
					document.getElementById("continuation").style.display = "none"
						document.getElementById("two").style.display = "none"
				}
				if(k==0){
					document.getElementById("freezeCancelInfo").style.display = "none"
						document.getElementById("three").style.display = "none"
				}
				if(g==0){
					document.getElementById("invalid").style.display = "none"
						document.getElementById("four").style.display = "none"
				}
			}
		})
        document.getElementById("bg").style.display ="block";
        document.getElementById("freeze_detail").style.display ="block";
	}
	//司法股权冻结信息中的状态为“冻结”
	function freezeDetailContinuation(parent_Id){
		if(freezeTemp == false){
		$.ajax({
			type:"GET",
			url:judiciaryStockfreezeDetailUrl,
			dataType:"JSON",
			async: true,
			success : function(data){
				var keepIsNull = true;
				for(var i=0;i<data.data.length;i++){
					if(data.data[i].parent_Id == parent_Id){
					if(data.data[i].frozState == "1"  && data.data[i].keepFrozDeadline == ""){
						var date1 = new Date(data.data[i].froFrom);
						var froFrom = date1.getFullYear()+'年'+(date1.getMonth()+1)+'月'+date1.getDate()+'日';
						var date2 = new Date(data.data[i].froTo);
						var froTo = date2.getFullYear()+'年'+(date2.getMonth()+1)+'月'+date2.getDate()+'日';		
						var date3 = new Date(data.data[i].publicDate);
						var publicDate = date3.getFullYear()+'年'+(date3.getMonth()+1)+'月'+date3.getDate()+'日';
						data.data[i].publicDate
						$("#baseDetail").append("<tr><td class='entry'>"+data.data[i].froAuth+"</td><td class='entry'>"+data.data[i].executeItem_CN+"</td><td class='entry'>"+data.data[i].froDocNo+"</td><td class='entry'>"+data.data[i].executeNo+"</td><td class='entry'>"+data.data[i].inv+"</td><td class='entry'>"+data.data[i].froAm+"</td><td class='entry'>"+data.data[i].cerType_CN+"</td><td class='entry'>"+data.data[i].cerNo+"</td><td class='entry'>"+froFrom+"</td><td class='entry'>"+froTo+"</td><td class='entry'>"+data.data[i].frozDeadline+"</td><td class='entry'>"+publicDate+"</td></tr>");
					}
					if(data.data[i].frozState == "1"  && data.data[i].keepFrozDeadline != ""){
						var date1 = new Date(data.data[i].thawDate);
						var thawDate = date1.getFullYear()+'年'+(date1.getMonth()+1)+'月'+date1.getDate()+'日';
						var date2 = new Date(data.data[i].publicDate);
						var publicDate = date2.getFullYear()+'年'+(date2.getMonth()+1)+'月'+date2.getDate()+'日';	
						$("#continuation").append("<tr><td class='entry'>"+data.data[i].froAuth+"</td><td class='entry'>"+data.data[i].executeItem_CN+"</td><td class='entry'>"+data.data[i].froDocNo+"</td><td class='entry'>"+data.data[i].executeNo+"</td><td class='entry'>"+data.data[i].inv+"</td><td class='entry'>"+data.data[i].froAm+"</td><td class='entry'>"+data.data[i].cerType_CN+"</td><td class='entry'>"+data.data[i].cerNo+"</td><td class='entry'>"+thawDate+"</td><td class='entry'>"+publicDate+"</td></tr>");
						keepIsNull = false;
					} 
				}
				}
				if(keepIsNull){				
					document.getElementById("continuation").style.display = "none"
				}
			}
		})
		freezeTemp = true;
		}
		$("#freeze_detail_continuation").show();
	}
	//司法股权冻结信息中的状态为“失效”
	function freezeDetailInvalid(parent_Id) {
		$.ajax({
			type: "GET",
			url: judiciaryStockfreezeDetailUrl,
			dataType: "JSON",
			async: true,
			success: function (data) {
				for (var i = 0; i < data.data.length; i++) {
					if (data.data[i].parent_Id == parent_Id) {
						if (data.data[i].frozState == "1" && data.data[i].keepFrozDeadline == "") {
							var date1 = new Date(data.data[i].froFrom);
							var froFrom = date1.getFullYear() + '年' + (date1.getMonth() + 1) + '月' + date1.getDate() + '日';
							var date2 = new Date(data.data[i].froTo);
							var froTo = date2.getFullYear() + '年' + (date2.getMonth() + 1) + '月' + date2.getDate() + '日';
							var date3 = new Date(data.data[i].publicDate);
							var publicDate = date3.getFullYear() + '年' + (date3.getMonth() + 1) + '月' + date3.getDate() + '日';
							data.data[i].publicDate
							$("#baseDetailInvalid").append("<tr><td class='entry'>" + data.data[i].froAuth + "</td><td class='entry'>" + data.data[i].executeItem_CN + "</td><td class='entry'>" + data.data[i].froDocNo + "</td><td class='entry'>" + data.data[i].executeNo + "</td><td class='entry'>" + data.data[i].inv + "</td><td class='entry'>" + data.data[i].froAm + "</td><td class='entry'>" + data.data[i].cerType_CN + "</td><td class='entry'>" + data.data[i].cerNo + "</td><td class='entry'>" + froFrom + "</td><td class='entry'>" + froTo + "</td><td class='entry'>" + data.data[i].frozDeadline + "</td><td class='entry'>" + publicDate + "</td></tr>");
						}
						if (data.data[i].frozState == "3") {
							var date1 = new Date(data.data[i].loseEffDate);
							var loseEffDate = date1.getFullYear() + '年' + (date1.getMonth() + 1) + '月' + date1.getDate() + '日';
							$("#Invalid").append("<tr><td class='entry'>" + data.data[i].loseEffRes_CN + "</td><td class='entry'>" + loseEffDate + "</td></tr>");
						}
					}
				}
			}
		})
		$("#freeze_detail_invalid").show();
	}
	function altershareholder(impOrgId){
		$.ajax({
			type:"GET",
			url:stakQualitDetailInfoUrl,
			dataType:"JSON",
			async: true,
			success : function(data){
				for(var i=0;i<data.data.length;i++){
					if(data.data[i].impOrgId == impOrgId){
						var altDate = convertDate(data.data[i].altDate);
						$("#holder_alter_altDate").html(altDate);
						$("#holder_alter_alt").html(data.data[i].alt);
					}
				}
			}
		})
		//$("#holder_alter").show();
        document.getElementById("bg").style.display ="block";
        document.getElementById("holder_alter").style.display ="block";
	}
	
