								}else if(row.alitem == "隶属企业名称"){
									$("#dependForColor2").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>隶属企业名称：</a>");
								}else if(row.alitem == "成员人数中农民人数"){
									$("#SfcfarnumForColor3").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>成员人数中农民人数：</a>");
								}else if(row.alitem == "本年度新增成员人数"){
									$("#SfcannNewMemNumForColor3").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>本年度新增成员人数：</a>");
								}else if(row.alitem == "本年度退出成员人数"){
									$("#SfcannRedMemNumForColor3").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>本年度退出成员人数：</a>");
								}else if(row.alitem == "经营者姓名"){
									$("#pbNameForColor4").html("<a href='#modifyMark' style='color:#FF0000;font-size:14px;'>经营者姓名：</a>");
